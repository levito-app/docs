{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Levito.app",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Levito.app",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
